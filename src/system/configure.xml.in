<?xml version="1.0"  encoding="UTF-8" ?>
<configure version="1">
  <!--
      Default dictionary directory are:
          Linux/Unix: "/user/local/share/AlphaDict/dicts/"
          Windows: "installing dir/dicts"
  -->
  <dir></dir>
  <srclan>any</srclan>
  <detlan>any</detlan>
  <dict>
    <!--
      <d open="" en=""/>
        <name/>
        <srclan/>
        <detlan/>
        <summary/>
      </d>
 
      @open : which inner class of AlphaDict open this dict. 
              'none' means it's not supported.
      @en: {true, false}
      @name : dictionary's name.
    -->
  </dict>
</configure>
